{
  "name": "chillu/silverstripe-graphql",
  "description": "GraphQL server for SilverStripe models and other data",
  "type": "silverstripe-module",
  "require": {
    "php": ">=5.5.0",
    "silverstripe/framework": "^4",
    "webonyx/graphql-php": "~0.7.2"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Chillu\\GraphQL\\": "src/"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
